{
  "kind": "collectionType",
  "collectionName": "sponsors",
  "info": {
    "singularName": "sponsor",
    "pluralName": "sponsors",
    "displayName": "Sponsor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::child.child",
      "mappedBy": "sponsor"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
